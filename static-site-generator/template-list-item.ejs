<% var templateMap = require('./temp/templateMap'); %>
<!DOCTYPE html>
<html lang="<%= htmlWebpackPlugin.options.lang || 'en' %>">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%= htmlWebpackPlugin.options.title || '' %></title>
</head>
<body>
  <div id="content">
    <%= require(`./src/views/partials/backLinkTitle.js`)(htmlWebpackPlugin.options.title, htmlWebpackPlugin.options.filename, templateMap) %>
    <%= require(`./src/views/partials/navLinks.js`)(htmlWebpackPlugin.options.previous, htmlWebpackPlugin.options.next) %>
    <%= require(`./src/views/${htmlWebpackPlugin.options.target}`) %>
  </div>
</body>
</html>