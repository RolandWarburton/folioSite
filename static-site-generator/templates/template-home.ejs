<% var path = require('path'); %>
<% var routeMap = require('../temp/routeMap'); %>
<% var title = htmlWebpackPlugin.options.title; %>
<% var filename = htmlWebpackPlugin.options.filename; %>

<!DOCTYPE html>
<html lang="<%= htmlWebpackPlugin.options.lang || 'en' %>">

<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title><%= htmlWebpackPlugin.options.title || '' %></title>
</head>

<body>
	<div id="content">
		<!-- <%= require(`../src/views/partials/router.js`) %> -->
		<%= require(`../src/views/partials/backLinkTitle.js`)(title, filename, routeMap) %>
		<%= require(`../src/views/${htmlWebpackPlugin.options.target}`) %>
	</div>
</body>

</html>

